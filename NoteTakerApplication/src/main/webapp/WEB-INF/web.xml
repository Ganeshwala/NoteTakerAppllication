<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>saveContentServelt</servlet-name>
  	<display-name>saveContentServelt</display-name>
  	<description></description>
  	<servlet-class>com.noteTaker.servlet.saveContentServelt</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>deleteNoteServlet</servlet-name>
  	<display-name>deleteNoteServlet</display-name>
  	<description></description>
  	<servlet-class>com.noteTaker.servlet.deleteNoteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>saveContentServelt</servlet-name>
  	<url-pattern>/saveContentServelt</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>deleteNoteServlet</servlet-name>
  	<url-pattern>/deleteNoteServlet</url-pattern>
  </servlet-mapping>
</web-app>
